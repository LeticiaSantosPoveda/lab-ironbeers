<div class="beerRandom">
{{#each randomBeers}}
    {{> beerpartial}}
    <div id="extrasRandom">
        <section id="foodPairing">
            <h2>Food Pairing</h2>
            <ul>
                {{#each this.food_pairing}}
                <li>{{this}}</li>
                {{/each}}
            </ul>
        </section>
        <section id="tips">
            <p><span id="span">Brewers Tips: </span>{{this.brewers_tips}}</p> 
        </section>  
    </div>     
{{/each}}
</div>